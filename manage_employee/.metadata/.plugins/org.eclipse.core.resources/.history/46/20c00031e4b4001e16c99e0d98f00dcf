package huana.com.vn.manage_employees;

import java.time.LocalDate;

import javax.xml.crypto.Data;

public class Employee {
	private Integer id;
	private String name;
	private String address;
	private String email;
	private String phone;
	private Data dateStartWork;
	private Double Salary;
	public Employee(Integer id, String name, String address, String email, String phone, Data dateStartWork,
			Double salary) {
		this.id = id;
		this.name = name;
		this.address = address;
		this.email = email;
		this.phone = phone;
		this.dateStartWork = dateStartWork;
		Salary = salary;
	}
	
}
